<?xml version="1.0" encoding="UTF-8"?>
<context>
	<Resource 
	name="jdbc/snow"
	auth="Container" 
	type="javax.sql.DataSource"
	username="system"
	password="oracle"
	driverClassName="oracle.jdbc.driver.OracleDriver"
	factory="org.apache.tomcat.dbcp.dbcp.BasicDataSourceFactory"
	url="jdbc:oracle:thin:@localhost:1521:XE"
	maxActive="500"
	maxIdle="100"
	/>
</context>
